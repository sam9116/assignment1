<?xml version="1.0" encoding="utf-8"?>

<manifest package="com.example.to_do_list"
    android:versionCode="1"
    android:versionName="1.0" xmlns:android="http://schemas.android.com/apk/res/android">

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="to_do_list.Taskholder"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:theme="@android:style/Theme.Dialog"
            android:name="to_do_list.Singleitem_dialog_archive"
            android:noHistory="false"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:theme="@android:style/Theme.Dialog"
            android:name="to_do_list.Singleitem_dialog"
            android:label="@string/title_activity_singleitem_dialog" >
        </activity>
        <activity
            android:theme="@android:style/Theme.Dialog"
            android:name="to_do_list.report"
            android:label="@string/title_activity_singleitem_dialog" >
        </activity>
    </application>

</manifest>
